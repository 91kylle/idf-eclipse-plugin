<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.launchbar.ui.launchTargetTypeUI">
      <launchTargetTypeUI
            id="com.espressif.launch.serial.core.serialFlashTarget"
            labelProvider="com.espressif.launch.serial.ui.internal.SerialFlashLaunchTargetLabelProvider">
      </launchTargetTypeUI>
      <wizard2
            class="com.espressif.launch.serial.ui.internal.NewSerialFlashTargetWizard"
            icon="icons/espressif.png"
            id="com.espressif.launch.serial.core.serialFlashTarget"
            name="ESP32 Target">
      </wizard2>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="com.espressif.launch.serial.ui.internal.SerialFlashLaunchConfigTabGroup"
            id="com.espressif.launch.serial.ui.launchConfigurationTabGroup"
            type="com.espressif.launch.serial.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.launchbar.ui.launchBarUIContributions">
      <buildTabGroup
            launchConfigType="com.espressif.launch.serial.launchConfigurationType"
            launchDescriptorType="org.eclipse.cdt.debug.core.coreBuildDescriptorType"
            tabGroup="org.eclipse.cdt.launch.internal.corebuild.CoreBuildTabGroup">
      </buildTabGroup>
   </extension>

</plugin>
